{
  "name": "PositiveView",
  "version": "1.0.0",
  "description": "My wicked Nuxt.js project",
  "author": "Tomoyuki Shimizu",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "API_URL_BROWSER=https://api.calil.jp/ nuxt build",
    "build-prod": "API_URL_BROWSER=https://api.calil.jp/ NODE_ENV=production nuxt build",
    "build-dev": "API_URL_BROWSER=https://api.calil.jp/ NODE_ENV=development nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "API_URL_BROWSER=https://api.calil.jp/ nuxt generate",
    "generate-prod": "API_URL_BROWSER=https://api.calil.jp/ NODE_ENV=production nuxt generate",
    "generate-dev": "API_URL_BROWSER=https://api.calil.jp/ NODE_ENV=development nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "debug": "node --inspect node_modules/.bin/nuxt"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "firebase": "^6.5.0",
    "idle-vue": "^2.0.5",
    "nuxt": "^2.0.0",
    "retryx": "0.0.7",
    "vue-scrollto": "^2.15.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0"
  }
}
